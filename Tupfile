ifeq (@(MPLAYER),y)
: foreach $(objs-y) |> !cc |> %B.o {objs}
: foreach $(YASM-objs-y) |> yasm -f ELF %f |> %B.o {objs}
: foreach $(X86-objs-y) |> !cc |> x86-%B.o {objs}
: {objs} |> !ar |> libavcodec.a
endif
